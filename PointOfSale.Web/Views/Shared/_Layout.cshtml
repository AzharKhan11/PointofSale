    <!--Delete Confirmation Modal-->
    <div id="delete_confirm" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
                <div class="modal-header" id="model-header">
                    <h4 class="modal-title">Confirmation<br /></h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    <i class="fa fa-trash"></i> &nbsp;<label id="confirmation_label">Are you sure you want to delete this record?</label>

                </div>
                <div class="modal-footer">
                    <div class="form-group pull-right">
                        <button type="button" id="btn_delete_no" class="btn btn-danger" value="No" data-dismiss="modal">No</button>
                        <input type="submit" id="btn_delete_yes" value="Yes" class="btn btn-success" />
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /#wrapper -->
