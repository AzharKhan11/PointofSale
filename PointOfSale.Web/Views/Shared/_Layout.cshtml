    <!-- Text Editor SCRIPTS-->
    <script>
        $(function () {
            $('#summernote').summernote();
            $('#summernote_air').summernote({
                airMode: true
            });
        });
    </script>

    <script type="text/javascript">
        $(document).ready(function () {
            $(".searchable").select2();
            $('.datepicker').datepicker({
                format: "M dd, yyyy"
            }).datepicker("setDate", new Date());
            @if (Convert.ToString(TempData["successMessage"]) != null) {
        @: $.notify("@TempData["successMessage"]", 'success');
    }
            $('.se-pre-con').hide();
        });
        function fnLoadPage() {
            $('.se-pre-con').show();
        }
        $(document).ready(function () {
            $('#dataTable').DataTable();
        });
    </script>
